# Database Configuration
# Use a read-only user for safety. Example:
# DATABASE_URL=postgresql://readonly_user:password@host:5432/database_name
DATABASE_URL=

# OpenAI API Key
# Get your API key from https://platform.openai.com/api-keys
OPENAI_API_KEY=

# Input/Output Paths
TAGS_CSV_PATH=data/tags.csv
OUTPUT_CSV_PATH=output/tag_suggestions.csv

# Optional: LLM Arbiter (default: true)
USE_LLM=true

# Optional: Write to Database Table (default: false)
# If true, creates and populates lecture_tag_suggestions table
WRITE_TO_DB=false

# Optional: Model Configuration
EMBEDDING_MODEL=text-embedding-3-large
LLM_MODEL=gpt-4o-mini
